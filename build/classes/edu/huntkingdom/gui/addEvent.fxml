            <TextField fx:id="txtprice" />
            <TextField fx:id="txtnumber" />
            <TextField fx:id="txtdescription" />
            <HBox>
               <children>
                  <TextField fx:id="txtimage" />
                  <Button mnemonicParsing="false" onAction="#upload" prefHeight="25.0" prefWidth="108.0" style="-fx-background-color: #6DAC4FFF;" text="upload" />
               </children>
            </HBox>
            <DatePicker fx:id="combodateD" style="-fx-background-image: #6DAC4FFF;" />
            <DatePicker fx:id="combodateF" />
            <Button mnemonicParsing="false" onAction="#addevent" prefHeight="35.0" prefWidth="102.0" style="-fx-background-color: #6DAC4FFF;" text="Add" />
